{
  "name": "There is only Keith Stuart",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Replace all google writers with Keith Stuart",
  "permissions": [
    "http://*.theguardian.com/*",
    "https://*.theguardian.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.theguardian.com/*",
        "https://*.theguardian.com/*"
      ],
      "js": [
        "jquery.js",
        "replaceor.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "onelifeleft.gif"
  ]
}
